{
  "name": "@trevoreyre/ui",
  "version": "0.0.15",
  "description": "Vue UI component library",
  "main": "dist/ui.common.js",
  "unpkg": "dist/ui.umd.min.js",
  "keywords": [
    "vue",
    "component",
    "library",
    "UI"
  ],
  "author": "Trevor Eyre <trevoreyre@gmail.com> (https://trevoreyre.com)",
  "license": "MIT",
  "repository": "trevoreyre/ui",
  "bugs": "https://github.com/trevoreyre/ui/issues",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "serve": "vue-cli-service serve --port 3000",
    "build": "npm run build:index && npm run build:lib",
    "build:index": "node scripts/build-index.js",
    "build:lib": "vue-cli-service build --target lib --name ui src/index.js",
    "build:watch": "vue-cli-service build --target lib --name ui src/index.js --watch",
    "build:docs": "build-storybook -o docs",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 4000 --ci"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "dependencies": {
    "@trevoreyre/autocomplete-vue": "^2.0.2",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-cssresources": "^5.0.6",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/vue": "^5.0.5",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.4",
    "storybook-addon-styles": "^1.0.0",
    "vue-template-compiler": "^2.5.21"
  }
}
