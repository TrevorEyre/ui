{
  "name": "@trevoreyre/ui",
  "version": "0.0.5",
  "private": false,
  "main": "index.js",
  "files": [
    "*.json",
    "**/*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --port 3001",
    "lint": "vue-cli-service lint",
    "build": "node scripts/build",
    "vue-cli-build": "vue-cli-service build --target lib",
    "build:lib": "vue-cli-service build --target lib --name ui src/index.js",
    "build:HelloWorld": "vue-cli-service build --target lib --name HelloWorld --dest dist/HelloWorld src/components/HelloWorld.vue"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^3.0.5",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^7.0.0",
    "lint-staged": "^7.2.2",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
