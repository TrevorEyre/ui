{
  "name": "@trevoreyre/ui",
  "version": "0.0.17",
  "description": "Vue UI component library",
  "main": "dist/ui.common.js",
  "unpkg": "dist/ui.umd.min.js",
  "keywords": [
    "vue",
    "component",
    "library",
    "UI"
  ],
  "author": "Trevor Eyre <trevoreyre@gmail.com> (https://trevoreyre.com)",
  "license": "MIT",
  "repository": "trevoreyre/ui",
  "bugs": "https://github.com/trevoreyre/ui/issues",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "start-storybook -p 4000 --ci",
    "serve": "vue-cli-service serve --port 3000",
    "build": "npm run build:index && npm run build:lib",
    "build:index": "node scripts/build-index.js",
    "build:lib": "vue-cli-service build --target lib --name ui src/index.js",
    "build:watch": "vue-cli-service build --target lib --name ui src/index.js --watch",
    "build:docs": "build-storybook -o docs",
    "lint": "vue-cli-service lint"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "dependencies": {
    "@trevoreyre/autocomplete-vue": "^2.0.3",
    "focus-visible": "^5.0.2",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-actions": "^5.3.0-beta.22",
    "@storybook/addon-docs": "^5.3.0-beta.22",
    "@storybook/addon-links": "^5.3.0-beta.22",
    "@storybook/addon-storysource": "^5.3.0-beta.22",
    "@storybook/addon-viewport": "^5.3.0-beta.22",
    "@storybook/vue": "^5.3.0-beta.22",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.7.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "lint-staged": "^9.5.0",
    "vue-template-compiler": "^2.5.21"
  }
}
