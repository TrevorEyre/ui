{
  "name": "@trevoreyre/ui",
  "version": "0.0.10",
  "main": "dist/ui.common.js",
  "unpkg": "dist/ui.umd.min.js",
  "author": "Trevor Eyre <trevoreyre@gmail.com> (https://trevoreyre.com)",
  "license": "MIT",
  "repository": "trevoreyre/ui",
  "bugs": "https://github.com/trevoreure/ui/issues",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "*.json",
    "**/*.js",
    "**/*.css",
    "**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --port 3001",
    "build": "npm run build:index && npm run build:lib",
    "build:index": "node scripts/build-index.js",
    "build:lib": "vue-cli-service build --target lib --name ui src/index.js",
    "lint": "vue-cli-service lint"
  },
  "peerDependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "fs-extra": "^7.0.0",
    "lint-staged": "^8.1.4",
    "vue-template-compiler": "^2.5.21"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
